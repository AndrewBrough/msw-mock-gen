{
  "name": "msw-mock-gen",
  "version": "1.0.0",
  "description": "A Vite plugin that automatically generates MSW (Mock Service Worker) handlers from your API calls by watching TypeScript/JavaScript files and parsing URL patterns",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write src/**/*.{ts,json,md}"
  },
  "keywords": [
    "vite",
    "plugin",
    "msw",
    "mock",
    "service-worker",
    "api-mocking",
    "typescript",
    "javascript",
    "development",
    "testing",
    "api",
    "endpoints",
    "handlers",
    "auto-generate",
    "file-watcher"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "@tanstack/react-query": "^5.37.1",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "glob": "^10.3.10",
    "msw": "^2.10.4",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "prettier-eslint": "^16.4.2",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  }
}
